{
  "manifest_version": 3,
  "name": "Bort",
  "version": "2.0",
  "author": "Osmo Salomaa",
  "description": "Bookmarking extension for Bort.io",
  "icons": {
    "16": "bort-16.png",
    "32": "bort-32.png",
    "48": "bort-48.png",
    "128": "bort-128.png"
  },
  "background": {
    "service_worker": "service_worker.js"
  },
  "permissions": ["activeTab"],
  "offline_enabled": false,
  "action": {
    "default_icon": {
      "19": "bort-19.png",
      "38": "bort-38.png"
    },
    "default_title": "Add Bort.io bookmark"
  },
  "content_security_policy": {}
}
